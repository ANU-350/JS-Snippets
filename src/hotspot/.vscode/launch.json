{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "/home/hamlin/workspace/repos/github/jdk/build/linux-riscv64-server-fastdebug/images/jdk/bin/java",
      "args": ["-XX:-TieredCompilation", "-Xbatch", "-Xcomp", "UnsignedTest"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": ["QEMU_LD_PREFIX=/rivos/sysroot/riscv/" "QEMU_CPU=rivos-sentinel-ga0"],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description": "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ]
    }

  ]
}