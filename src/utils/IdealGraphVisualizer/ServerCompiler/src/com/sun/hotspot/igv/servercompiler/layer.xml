<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
      <folder name="Filters">
        <file name="Color" url="filters/color.filter">
            <attr name="enabled" boolvalue="true"/>
        </file>
        <file name="Remove secondary edges" url="filters/remove.filter">
            <attr name="enabled" boolvalue="false"/>
            <attr name="after" stringvalue="Color"/>
        </file>
        <file name="Simplify structure" url="filters/structural.filter">
            <attr name="enabled" boolvalue="false"/>
            <attr name="after" stringvalue="Remove secondary edges"/>
        </file>
        <file name="Show only control flow" url="filters/onlyControlFlow.filter">
            <attr name="enabled" boolvalue="false"/>
            <attr name="after" stringvalue="Simplify structure"/>
        </file>
    </folder>
</filesystem>
